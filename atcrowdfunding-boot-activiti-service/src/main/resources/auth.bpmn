    </endEvent>
    <sequenceFlow id="flow15" name="审核通过" sourceRef="exclusivegateway4" targetRef="endevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${falg==true}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent2" name="End">
      <extensionElements>
        <activiti:executionListener event="end" class="com.atguigu.atcrowdfunding.listener.RefuseListener"></activiti:executionListener>
      </extensionElements>
    </endEvent>
